{
  "name": "blog-api",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "migration:create": "node --env-file .env umzug/migrate create --name",
    "migrate": "node --env-file .env umzug/migrate up",
    "migrate:rollback": "node --env-file .env umzug/migrate down",
    "migrate:rollback:all": "node --env-file .env umzug/migrate down --to 0",
    "seed": "node --env-file .env umzug/seed up",
    "seed:create": "node --env-file .env umzug/seed create --name",
    "seed:rollback": "node --env-file .env umzug/seed down",
    "seed:rollback:all": "node --env-file .env umzug/seed down --to 0",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "dev": "nodemon --watch ./src/*/** --ext js,mjs,jsx,json --exec 'node --env-file .env server.js'",
    "start": "node server.js"
  },
  "author": "Ahmad Abudawba",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.535.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.3",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "next": "^14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "sequelize": "^6.37.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@remixicon/react": "^4.2.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "umzug": "^3.7.0"
  }
}
